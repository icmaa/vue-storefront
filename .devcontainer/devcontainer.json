{
  "name": "icmaa/vue-storefront",
  "image": "mcr.microsoft.com/vscode/devcontainers/javascript-node:16",
  "forwardPorts": [
    3000
  ],
  "remoteEnv": {
    "NODE_APP_INSTANCE": "imp",
    "NODE_CONFIG_ENV": "development",
    "NODE_TLS_REJECT_UNAUTHORIZED": "0"
  },
  "postCreateCommand": [
    "bash", ".devcontainer/scripts/postCreate.sh"
  ]
}
