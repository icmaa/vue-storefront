    "webpack-hot-middleware": "^2.24.3",
    "webpack-merge": "^4.2.2",
    "workbox-webpack-plugin": "^6.2.3"
  },
  "peerDependencies": {
    "vue-template-compiler": "^2.6.14"
  },
  "browserslist": {
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 edge versions",
      "> 5%"
    ],
    "production": [
      "> 5%",
      "last 2 versions",
      "Firefox ESR",
      "not IE 11",
      "not dead"
    ]
  },
  "workspaces": [
    "core",
    "core/i18n",
    "src/extensions/*",
    "src/modules/*",
    "src/trace/*",
    "src/themes/*",
    "src/themes/*/test/*",
    "docs",
