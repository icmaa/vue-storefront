{
  "name": "vue-storefront",
  "version": "1.12.3",
  "description": "A Vue.js, PWA eCommerce frontend",
  "private": true,
  "engines": {
    "node": "^16",
    "yarn": "^1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vuestorefront/vue-storefront"
  },
  "keywords": [
    "vue",
    "vuex",
    "vue-router",
    "webpack",
    "starter",
    "server-side",
    "boilerplate"
  ],
  "author": "pkarw <pkarwatka@divante.pl> and contributors",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vuestorefront/vue-storefront/issues"
  },
  "homepage": "https://github.com/vuestorefront/vue-storefront/README.md",
  "scripts": {
    "static-server": "cross-env TS_NODE_PROJECT=\"tsconfig-build.json\" ts-node ./core/scripts/static-server.ts",
    "generate": "cross-env TS_NODE_PROJECT=\"tsconfig-build.json\" ts-node ./core/scripts/generate.ts",
    "generate-files": "cross-env TS_NODE_PROJECT=\"tsconfig-build.json\" ts-node ./core/scripts/generate-files.ts",
    "start": "cross-env NODE_ENV=production node ./core/scripts/server.h3.js",
    "start:inspect": "cross-env NODE_ENV=production node --inspect ./core/scripts/server.h3.js",
    "start:pm2": "cross-env NODE_ENV=production pm2-runtime -i 4 ./core/scripts/server.h3.js",
    "dev": "tsc --build --clean && yarn generate-files && cross-env TS_NODE_PROJECT=\"tsconfig-build.json\" ts-node ./core/scripts/server.h3",
    "dev:inspect": "tsc --build --clean && yarn generate-files && cross-env TS_NODE_PROJECT=\"tsconfig-build.json\" node --inspect -r ts-node/register ./core/scripts/server.h3",
    "build:client-bundle": "cross-env NODE_ENV=production TS_NODE_PROJECT=\"tsconfig-build.json\" webpack --config ./core/build/webpack.prod.client.config.ts --mode production --progress --hide-modules",
    "build:server-bundle": "cross-env NODE_ENV=production TS_NODE_PROJECT=\"tsconfig-build.json\" webpack --config ./core/build/webpack.prod.server.config.ts --mode production --progress --hide-modules",
    "build:server": "tsc -P tsconfig-build-production.json",
    "build": "rimraf dist && tsc --build --clean && yarn generate-files && yarn build:client-bundle && yarn build:server-bundle && yarn build:server",
    "test:unit": "jest -c test/unit/jest.conf.js",
    "test:unit:watch": "jest -c test/unit/jest.conf.js --watch",
    "lint": "cross-env NODE_ENV=production  eslint --ext .js,.vue,.ts core src --fix",
    "lerna": "lerna"
  },
  "dependencies": {
    "@babel/runtime": "^7.17.9",
    "@types/webpack": "^4.41",
    "@types/webpack-dev-server": "^3.10",
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-client": "^2.6.8",
    "apollo-link": "^1.2.13",
    "apollo-link-http": "^1.5.16",
    "app-root-path": "^2.0.1",
    "body-scroll-lock": "^2.6.4",
    "bodybuilder": "2.2.21",
    "config": "^1.30.0",
    "cross-env": "^3.1.4",
    "dayjs": "^1.8.21",
    "detect-installed": "^2.0.4",
    "es6-promise": "^4.2.8",
    "express": "^4.14.0",
    "fork-ts-checker-webpack-plugin": "^6.2.0",
    "fs-extra": "^8.1.0",
    "glob": "^7.1.6",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.10.3",
    "h3": "^0.7.8",
    "intl": "^1.2.5",
    "intl-locales-supported": "^1.8.2",
    "isomorphic-fetch": "^2.2.1",
    "js-sha3": "^0.8.0",
    "localforage": "^1.7.2",
    "magento2-rest-client": "github:DivanteLtd/magento2-rest-client",
    "mime": "^3.0.0",
    "ms": "^2.1.2",
    "pm2": "^5.2.0",
    "proxy-polyfill": "^0.3.0",
    "redis-tag-cache": "^1.2.1",
    "reflect-metadata": "^0.1.12",
    "serve-static": "^1.14.2",
    "storefront-query-builder": "https://github.com/vuestorefront/storefront-query-builder.git",
    "ts-node": "^8.6.2",
    "vue": "^2.6.14",
    "vue-analytics": "^5.16.1",
    "vue-apollo": "^3.0.3",
    "vue-gtm": "^2.0.0",
    "vue-i18n": "^8.0.0",
    "vue-lazy-hydration": "^1.0.0-beta.9",
    "vue-meta": "^2.4.0",
    "vue-no-ssr": "^1.1.1",
    "vue-offline": "^1.0.8",
    "vue-router": "3.1.6",
    "vue-server-renderer": "^2.6.14",
    "vuelidate": "^0.7.5",
    "vuex": "^3.5.1",
    "vuex-router-sync": "^5.0.0",
    "workbox-expiration": "^6.2.3",
    "workbox-precaching": "^6.2.3",
    "workbox-routing": "^6.2.3",
    "workbox-strategies": "^6.2.3",
    "workbox-window": "^6.2.3"
  },
